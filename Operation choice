#include<stdio.h>
int operationchoice(int a,int b,int c){
    if(c==1){
        return a+b;
    }
    else if(c==2){
        return a-b;
        
    }
    else if(c==3){
        return a*b;
    }
    else if(c==4){
        return a/b;
    }
}
int main(){
    int a=12,b=16,c=2;
    int result=operationchoice(a,b,c);
    printf("%d",result);
}
